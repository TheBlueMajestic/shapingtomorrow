  <package id="2" version="1.3.0" url="http://www.eyecatch.no/projects/csv-generator/" name="EyeCatch CSV Generator" folder="9cc62295-aede-46fb-87b1-cba45b4b5885" packagepath="" repositoryGuid="" packageGuid="" hasUpdate="False" enableSkins="False" skinRepoGuid="00000000-0000-0000-0000-000000000000">

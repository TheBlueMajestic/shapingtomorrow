          <AccordionGroup id="1811" parentID="1801" level="5" creatorID="1" sortOrder="0" createDate="2015-11-12T10:29:10" updateDate="2015-11-12T10:29:10" nodeName="Electric Vehicles" urlName="electric-vehicles" path="-1,1052,1053,1532,1801,1811" isDoc="" nodeType="1177" creatorName="euser" writerName="euser" writerID="1" template="0" nodeTypeAlias="AccordionGroup">
